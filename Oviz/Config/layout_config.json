{
    "last_slide_num" : 0,
    "image_dock_path":
    [
        {
            "default_value":{
                "value":""
            }
        }
    ],
    "global_control_box":
    {
        "global_setting":
        {
            "type": "QVBoxLayout",
            "widget" : {
                "color_id_map_list" : {
                    "type": "self.create_color_map_widget",
                    "params": {}
                },
                "checkbox_show_grid" : {
                    "type" : "QCheckBox",
                    "params":{
                        "text": "显示grid"
                    }
                },
                "grid_line_x_range":{
                    "type": "LineTextWithLabelWidget",
                    "params":{
                        "widget_titie" : "x_range",
                        "default_value": {
                            "value": "-70, 70"
                        }
                    }
                },

                "grid_line_y_range":{
                    "type": "LineTextWithLabelWidget",
                    "params":{
                        "widget_titie" : "y_range",
                        "default_value": {
                            "value": "-70, 70"
                        }
                    }
                },

                "grid_line_step":{
                    "type": "LineTextWithLabelWidget",
                    "params":{
                        "widget_titie" : "xy_step",
                        "default_value": {
                            "value": "10, 10"
                        }
                    }
                }
            }
        },

        "pointcloud":{
            "type": "QVBoxLayout",
            "widget":{
                "folder_path" : {
                    "type": "FolderSelectWidget",
                    "params":{
                        "widget_titie" : "pointcloud",
                        "default_value": {
                            "value": ""
                        }
                    }
                },

                "linetxt_point_dim":{
                    "type": "LineTextWithLabelWidget",
                    "params":{
                        "widget_titie" : "dim",
                        "default_value": {
                            "value": "4"
                        }
                    }
                },

                "linetxt_point_type":{
                    "type": "LineTextWithLabelWidget",
                    "params":{
                        "widget_titie" : "type",
                        "default_value": {
                            "value": "float32"
                        }
                    }
                },

                "linetxt_xyz_dim" : {
                    "type": "LineTextWithLabelWidget",
                    "params":{
                        "widget_titie" : "x,y,z",
                        "default_value": {
                            "value": "0,1,2"
                        }
                    }
                },

                "linetxt_wlh_dim" : {
                    "type": "LineTextWithLabelWidget",
                    "params":{
                        "widget_titie" : "w,l,h",
                        "default_value": {
                            "value": "-1"
                        }
                    }
                },

                "linetxt_color_dim" : {
                    "type": "LineTextWithLabelWidget",
                    "params":{
                        "widget_titie" : "color",
                        "default_value": {
                            "value": "3"
                        }
                    }
                }
            }
        },

        "record_screen_setting":
        {
            "type": "QVBoxLayout",
            "widget" : {
                "checkbox_record_screen":{
                    "type" : "QCheckBox",
                    "params":{
                        "text": "切帧事件录屏"
                    }
                },
                "checkbox_mouse_record_screen":{
                    "type" : "QCheckBox",
                    "params":{
                        "text": "鼠标事件录屏"
                    }
                },
                "folder_path" : {
                    "type": "FolderSelectWidget",
                    "params":{
                        "widget_titie" : "Save Dir",
                        "default_value": {
                            "value": "~/Videos"
                        }
                    }
                },
                "button_export_record_video":{
                    "type" : "QPushButton",
                    "params":{
                        "text": "导出视频"
                    }
                }
            }
        },

        "car_model_setting":
        {
            "type": "QVBoxLayout",
            "widget" : {
                "checkbox_show_car":{
                    "type" : "QCheckBox",
                    "params":{
                        "text": "显示车模型"
                    }
                }
            }
        }
    }
}
