{
    "image_flag": 0,
    "log_flag": 0,
    "slide_flag": 0,
    "last_slide_num" : 0,
    "status_bar_flag" : 1,
    "control_box_flag" : 1,
    "image_dock_config": {
        "rl": "top",
        "sl": "top",
        "fl": "top",
        "fc": "top",
        "fr": "top",
        "sr": "top"
        //"rr": "top"
    },
    "image_dock_path":
    {
        "rl": "",
        "sl": "",
        "fl": "",
        "fc": "",
        "fr": "",
        "sr": ""
        //"rr": ""
    },

    "global_control_box":
    {
        "global_setting":
        {
            "type": "QVBoxLayout",
            "widget" : {
                "color_id_map_list" : {
                    "type": "self.create_color_map_widget",
                    "params": {}
                },
                "checkbox_show_grid" : {
                    "type" : "QCheckBox",
                    "params":{
                        "text": "显示grid"
                    }
                },
                "checkbox_online_mode":{
                    "type" : "QCheckBox",
                    "params":{
                        "text": "在线模式"
                    }
                }
            }
        },

        "record_screen_setting":
        {
            "type": "QVBoxLayout",
            "widget" : {
                "checkbox_record_screen":{
                    "type" : "QCheckBox",
                    "params":{
                        "text": "切帧事件录屏"
                    }
                },
                "checkbox_mouse_record_screen":{
                    "type" : "QCheckBox",
                    "params":{
                        "text": "鼠标事件录屏"
                    }
                },
                "button_select_record_save" : {
                    "type": "FolderSelectWidget",
                    "params":{
                        "widget_titie" : "Save Dir",
                        "default_value": {
                            "value": "~/Videos"
                        }
                    }
                },
                "button_export_record_video":{
                    "type" : "QPushButton",
                    "params":{
                        "text": "导出视频"
                    }
                }
            }
        },

        "car_model_setting":
        {
            "type": "QVBoxLayout",
            "widget" : {
                "checkbox_show_car":{
                    "type" : "QCheckBox",
                    "params":{
                        "text": "显示车模型"
                    }
                }
            }
        },

        "magic_pipeline_setting":
        {
            "type": "QVBoxLayout",
            "widget" : {
                "checkbox_enable_magic":{
                    "type" : "QCheckBox",
                    "params":{
                        "text": "使用魔法pipeline"
                    }
                },
                "text_magic_pipe_paramters":{
                    "type" : "JSONWidgetEditor",
                    "params":{
                        "widget_titie" : "json",
                        "default_value": [{
                            "magic_debug" : 1
                        }]
                    }
                },
                "button_open_magic_pipe_editor":{
                    "type" : "QPushButton",
                    "params":{
                        "text": "打开编辑器"
                    }
                }
            }
        }
    },

    "element_control_box":
    {
        "template":
        {
            "point_setting":{
                "type": "QVBoxLayout",
                "widget":{
                    "linetxt_point_dim":{
                        "type": "LineTextWithLabelWidget",
                        "params":{
                            "widget_titie" : "dim",
                            "default_value": {
                                "value": "4"
                            }
                        }
                    },

                    "linetxt_point_type":{
                        "type": "LineTextWithLabelWidget",
                        "params":{
                            "widget_titie" : "type",
                            "default_value": {
                                "value": "float32"
                            }
                        }
                    },

                    "linetxt_xyz_dim" : {
                        "type": "LineTextWithLabelWidget",
                        "params":{
                            "widget_titie" : "x,y,z",
                            "default_value": {
                                "value": "0,1,2"
                            }
                        }
                    },

                    "linetxt_wlh_dim" : {
                        "type": "LineTextWithLabelWidget",
                        "params":{
                            "widget_titie" : "w,l,h",
                            "default_value": {
                                "value": "-1"
                            }
                        }
                    },

                    "linetxt_color_dim" : {
                        "type": "LineTextWithLabelWidget",
                        "params":{
                            "widget_titie" : "color",
                            "default_value": {
                                "value": "5"
                            }
                        }
                    },

                    "button_select_pointcloud" : {
                        "type": "FolderSelectWidget",
                        "params":{
                            "widget_titie" : "Point Cloud",
                            "default_value": {
                                "value": ""
                            }
                        }
                    },

                    "show_voxel_mode" : {
                        "type": "QCheckBox",
                        "params":{
                            "text": "voxel模式"
                        }
                    }
                }
            },

            "bbox3d_setting":{
                "type": "QVBoxLayout",
                "widget":{
                    "bbox3d_txt_xyzwhlt_dim":{
                        "type": "LineTextWithLabelWidget",
                        "params":{
                            "widget_titie" : "x,y,z,w,l,h,θ",
                            "default_value": {
                                "value": "1,2,3,4,5,6,7"
                            }
                        }
                    },

                    "bbox3d_txt_color_dim":{
                        "type": "LineTextWithLabelWidget",
                        "params":{
                            "widget_titie" : "color (cls/id)",
                            "default_value": {
                                "value": "0"
                            }
                        }
                    },

                    "bbox3d_txt_arrow_dim":{
                        "type": "LineTextWithLabelWidget",
                        "params":{
                            "widget_titie" : "arrow",
                            "default_value": {
                                "value": "-1"
                            }
                        }
                    },

                    "bbox3d_txt_text_dim":{
                        "type": "LineTextWithLabelWidget",
                        "params":{
                            "widget_titie" : "text_dim",
                            "default_value": {
                                "value": "-1"
                            }
                        }
                    },

                    "bbox3d_txt_format_dim":{
                        "type": "LineTextWithLabelWidget",
                        "params":{
                            "widget_titie" : "show_format",
                            "default_value": {
                                "value": "[ID: %d]"
                            }
                        }
                    },

                    "button_select_bbox3d" : {
                        "type": "FolderSelectWidget",
                        "params":{
                            "widget_titie" : "3D Bbox",
                            "default_value": {
                                "value": ""
                            }
                        }
                    }
                }
            },

            "lane3d_setting": {
                "type": "QVBoxLayout",
                "widget":{
                    "button_select_bbox3d" : {
                        "type": "FolderSelectWidget",
                        "params":{
                            "widget_titie" : "3D Lane",
                            "default_value": {
                                "value": ""
                            }
                        }
                    }
                }
            },

            "bbox2d_setting" : {
                "type": "QVBoxLayout",
                "widget":{
                    "button_select_bbox2d" : {
                        "type": "FolderSelectWidget",
                        "params":{
                            "widget_titie" : "2D Bbox",
                            "default_value": {
                                "value": ""
                            }
                        }
                    }
                }
            }
        }
    }
}
